<?xml version="1.0" encoding="UTF-8" ?>
<Shell
  x:Class="WoL.AppShell"
  xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
  xmlns:local="clr-namespace:WoL"
  Shell.FlyoutBehavior="Flyout">

  <FlyoutItem Title="Home" Route="Home">
    <ShellContent Title="WoL" ContentTemplate="{DataTemplate local:MainPage}" Route="MainPage" />
  </FlyoutItem>

  <FlyoutItem Title="Configuration" Route="Configuration">
    <ShellContent Title="Configuration" ContentTemplate="{DataTemplate local:ConfigurationPage}" Route="ConfigurationPage" />
  </FlyoutItem>
</Shell>
